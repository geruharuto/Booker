<h1>Editing Book</h1>
<%= form_for(@book) do |f| %>
    <h4>タイトル</h4>
    <%= f.text_field :title %>
    <h4>本文</h4>
    <%= f.text_area :body %>
    <%= link_to 'Update Book', book_path(@book.id) %>
    <%= link_to 'Show', book_path(@book.id) %>
    <%= link_to 'Back', books_path %>
<% end %>